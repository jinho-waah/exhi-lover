e, setState] = useState({\n    center: {\n      lat: 37.56649,\n      lng: 126.978488,\n    },\n    errMsg: null,\n    isLoading: true,\n  });\n\n  useEffect(() => {\n    if (navigator.geolocation) {\n      // GeoLocation을 이용해서 접속 위치를 얻어옵니다\n      navigator.geolocation.getCurrentPosition(\n        (position) => {\n          setState((prev) => ({\n            ...prev,\n            center: {\n              lat: position.coords.latitude, // 위도\n              lng: position.coords.longitude, // 경도\n            },\n            isLoading: false,\n          }));\n        },\n        (err) => {\n          setState((prev) => ({\n            ...prev,\n            errMsg: err.message,\n            isLoading: false,\n          }));\n        }\n      );\n    } else {\n      // HTML5의 GeoLocation을 사용할 수 없을때 마커 표시 위치와 인포윈도우 내용을 설정합니다\n      setState((prev) => ({\n        ...prev,\n        errMsg: \"geolocation을 사용할수 없어요..\",\n        isLoading: false,\n      }));\n    }\n  }, []);\n\n  return (\n    <MapBox>\n      <Map state={state.center} />\n    </MapBox>\n  );\n};\n\nexport default React.memo(MapTemplate);\n","/Users/andrew/Desktop/exhi/exhi/client/src/component/hashtag/HashTag.js",[],[],[],"/Users/andrew/Desktop/exhi/exhi/client/src/component/search/Search.js",[],[],"/Users/andrew/Desktop/exhi/exhi/client/src/component/hashtag/Tags.js",[],[],"/Users/andrew/Desktop/exhi/exhi/client/src/component/hashtag/HashTagTemplate.js",[],[],"/Users/andrew/Desktop/exhi/exhi/client/src/component/hashtag/TagsCheckBox.js",["202","203","204"],[],"import * as React from \"react\";\nimport { useState, useEffect } from \"react\";\nimport Done from \"@mui/icons-material/Done\";\nimport styled from \"styled-components\";\nimport { fetchEnableTagId } from \"../../lib/api/Api\";\n\nconst Box = styled.div`\n  background-color: #000000;\n  width: 95%;\n  padding-left: 0.9rem;\n  display: flex;\n  flex-direction: column;\n  justify-content: flex-start;\n  justify-content: space-between;\n  align-items: f